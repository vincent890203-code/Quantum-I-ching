[
  {
    "number": 1,
    "name": "乾",
    "judgment": "元亨利貞。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：潛龍，勿用。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：見龍再田，利見大人。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "九三：君子終日乾乾，夕惕若，厲無咎。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "九四：或躍在淵，無咎。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：飛龍在天，利見大人。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上九：亢龍有悔。",
        "xiang": ""
      },
      {
        "position": 7,
        "meaning": "用九：見群龍無首，吉。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 2,
    "name": "坤",
    "judgment": "元亨，利牝馬之貞。君子有攸往，先迷，後得主，利。西南得朋，東北喪朋。安貞吉。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初六：履霜，堅冰至。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "六二：直方大，不習無不利。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：含章可貞；或從王事，無成有終。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：括囊，無咎無譽。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "六五：黃裳，元吉。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上六：龍戰於野，其血玄黃。",
        "xiang": ""
      },
      {
        "position": 7,
        "meaning": "用六：利永貞。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 3,
    "name": "屯",
    "judgment": "元亨，利貞；勿用有攸往，利建侯。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：磐桓；利居貞，利建侯。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "六二：屯如，邅如，乘馬班如。匪寇婚媾。女子貞不字，十年乃字。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：即鹿無虞，惟入於林中，君子幾，不如舍，往吝。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：乘馬班如，求婚媾，往吉，無不利。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：屯其膏，小貞吉，大貞兇。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上六：乘馬班如，泣血漣如。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 4,
    "name": "蒙",
    "judgment": "亨。匪我求童蒙，童蒙求我。初筮告，再三瀆，瀆則不告。利貞。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初六：發矇，利用刑人，用說桎梏；以往吝。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：包蒙，吉。納婦，吉；子克家。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：勿用取女，見金夫，不有躬，無攸利。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：困蒙，吝。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "六五：童蒙，吉。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上九：擊蒙；不利為寇，利禦寇。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 5,
    "name": "需",
    "judgment": "有孚，光亨，貞吉，利涉大川。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：需於郊，利用恆，無咎。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：需於沙，小有言；終吉。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "九三：需於泥，致寇至。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：需於血，出自穴。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：需於酒食，貞吉。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上六：入於穴，有不速之客三人來；敬之，終吉。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 6,
    "name": "訟",
    "judgment": "有孚，窒惕，中吉；終兇。利見大人，不利涉大川。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初六：不永所事，小有言，終吉。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：不克訟，歸而逋，其邑人三百戶，無眚。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：食舊德，貞厲，終吉；或從王事，無成。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "九四：不克訟；復即命，渝，安貞，吉。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：訟，元吉。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上九：或錫之鞶帶，終朝三褫之。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 7,
    "name": "師",
    "judgment": "貞，丈人吉，無咎。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初六：師出以律，否臧兇。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：在師中吉，無咎；王三錫命。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：師或輿尸，兇。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：師左次，無咎。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "六五：田有禽，利執言，無咎；長子帥師，弟子輿尸，貞兇。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上六：大君有命，開國承家，小人勿用。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 8,
    "name": "比",
    "judgment": "吉。原筮，元永貞，無咎。不寧方來，後夫兇。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初六：有孚比之，無咎；有孚盈缶，終來有它，吉。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "六二：比之自內，貞吉。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：比之匪人。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：外比之，貞吉。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：顯比；王用三驅，失前禽。邑人不誡，吉。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上六：比之無首，兇。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 9,
    "name": "小畜",
    "judgment": "亨。密雲不雨，自我西郊。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：復自道，何其咎？吉。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：牽復，吉。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "九三：輿說輻，夫妻反目。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：有孚，血去惕出，無咎。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：有孚攣如，富以其鄰。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上九：既雨既處，尚德載；婦貞厲，月幾望；君子徵兇。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 10,
    "name": "履",
    "judgment": "履虎尾，不咥人，亨。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：素履，往，無咎。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：履道坦坦，幽人貞吉。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：眇能視，跛能履，履虎尾，咥人，兇；武人為於大君。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "九四：履虎尾，愬愬，終吉。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：夬履，貞厲。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上九：視履考祥，其旋元吉。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 11,
    "name": "泰",
    "judgment": "小往大來，吉，亨。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：拔茅茹，以其匯，徵吉。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：包荒，用馮河，不遐遺；朋亡，得尚於中行。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "九三：無平不陂，無往不復；艱貞無咎，勿恤其孚，於食有福。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：翩翩，不富，以其鄰，不戒以孚。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "六五：帝乙歸妹，以祉元吉。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上六：城復於隍，勿用師，自邑告命，貞吝。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 12,
    "name": "否",
    "judgment": "否之匪人，不利君子貞，大往小來。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初六：拔茅茹，以其匯，貞吉，亨。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "六二：包承，小人吉。大人否，亨。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：包羞。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "九四：有命無咎，疇離祉。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：休否，大人吉；其亡其亡，繫於苞桑。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上九：傾否，先否後喜。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 13,
    "name": "同人",
    "judgment": "同人於野，亨。利涉大川，利君子貞。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：同人於門，無咎。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "六二：同人於宗，吝。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "九三：伏戎於莽，升其高陵，三歲不興。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "九四：乘其墉，弗克攻，吉。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：同人，先號咷，而後笑，大師克相遇。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上九：同人於郊，無悔。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 14,
    "name": "大有",
    "judgment": "元亨。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：無交害，匪咎；艱則無咎。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：大車以載，有攸往，無咎。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "九三：公用亨於天子，小人弗克。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "九四：匪其彭，無咎。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "六五：厥孚交如，威如，吉。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上九：自天祐之，吉無不利。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 15,
    "name": "謙",
    "judgment": "亨，君子有終。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初六：謙謙君子，用涉大川，吉。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "六二：鳴謙，貞吉。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "九三：勞謙，君子有終，吉。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：無不利，撝謙。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "六五：不富以其鄰，利用侵伐，無不利。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上六：鳴謙，利用行師，徵邑國。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 16,
    "name": "豫",
    "judgment": "利建侯，行師。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初六：鳴豫，兇。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "六二：介於石，不終日，貞吉。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：盱豫，悔。遲有悔。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "九四：由豫，大有得；勿疑。朋盍簪。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "六五：貞疾，恆不死。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上六：冥豫，成有渝，無咎。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 17,
    "name": "隨",
    "judgment": "元亨，利貞，無咎。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：官有渝，貞吉；出門交有功。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "六二：系小子，失丈夫。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：系丈夫，失小子；隨有求得，利居貞。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "九四：隨有獲，貞兇；有孚在道，以明，何咎？",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：孚於嘉，吉。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上六：拘繫之，乃從維之；王用亨於西山。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 18,
    "name": "蠱",
    "judgment": "元亨，利涉大川。先甲三日，後甲三日。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初六：幹父之蠱，有子，考無咎，厲終吉。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：幹母之蠱，不可貞。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "九三：幹父之蠱，小有悔，無大咎。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：裕父之蠱，往見吝。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "六五：幹父之蠱，用譽。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上九：不事王侯，高尚其事。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 19,
    "name": "臨",
    "judgment": "元亨，利貞；至於八月，有兇。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：鹹臨，貞吉。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "九二：鹹臨，吉，無不利。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：甘臨，無攸利；既憂之，無咎。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：至臨，無咎。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "六五：知臨，大君之宜，吉。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上六：敦臨，吉，無咎。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 20,
    "name": "觀",
    "judgment": "盥而不薦，有孚顒若。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初六：童觀，小人無咎，君子吝。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "六二：窺觀，利女貞。",
        "xiang": ""
      },
      {
        "position": 3,
        "meaning": "六三：觀我生，進退。",
        "xiang": ""
      },
      {
        "position": 4,
        "meaning": "六四：觀國之光，利用賓於王。",
        "xiang": ""
      },
      {
        "position": 5,
        "meaning": "九五：觀我生，君子無咎。",
        "xiang": ""
      },
      {
        "position": 6,
        "meaning": "上九：觀其生，君子無咎。",
        "xiang": ""
      }
    ]
  },
  {
    "number": 21,
    "name": "噬嗑",
    "judgment": "亨，利用獄。",
    "image": "",
    "lines": [
      {
        "position": 1,
        "meaning": "初九：屨校滅趾，無咎。",
        "xiang": ""
      },
      {
        "position": 2,
        "meaning": "六二：噬膚滅鼻，無咎。",
        "xiang": ""
      },
      {
        "position": 3,
